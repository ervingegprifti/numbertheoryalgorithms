    <!-- Common Styles -->
    <style name="CommonStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ButtonStyle" parent="CommonStyle">
        <item name="cornerRadius">5dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
    </style>


    <!-- Calculator Styles -->
    <style name="CalculatorButtonStyle" parent="ButtonStyle">
        <item name="backgroundTint">@color/button_run_bg_selector</item>
        <item name="android:textColor">@color/buttonRunTextColor</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginStart">0dp</item>
        <item name="android:layout_marginEnd">7dp</item>
        <item name="android:padding">0dp</item>
    </style>
    <style name="CalculatorButtonLastStyle" parent="CalculatorButtonStyle">
        <item name="android:layout_marginEnd">0dp</item>
    </style>


    <style name="ButtonRunStyle" parent="ButtonStyle">
        <item name="backgroundTint">@color/button_run_bg_selector</item>
        <item name="android:textColor">@color/buttonRunTextColor</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:paddingVertical">0dp</item>
        <item name="android:paddingHorizontal">10dp</item>
    </style>


    <!-- Button Example -->
    <style name="ButtonExampleStyle" parent="ButtonStyle">
        <item name="backgroundTint">@color/button_example_bg_selector</item>
        <item name="android:textColor">@color/buttonExampleTextColor</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginStart">5dp</item>
