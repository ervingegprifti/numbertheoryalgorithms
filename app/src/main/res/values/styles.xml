    <style name="ButtonExampleFirstStyle" parent="ButtonExampleStyle">
        <item name="android:layout_marginStart">0dp</item>
    </style>
    <style name="ButtonExampleLastStyle" parent="ButtonExampleStyle">
        <item name="android:layout_marginEnd">0dp</item>
    </style>
    <style name="ButtonExampleAfterRunStyle" parent="ButtonExampleStyle">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_weight">0</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginEnd">0dp</item>
    </style>



    <!-- Button Pills -->
    <style name="ButtonPillStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:padding">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_weight">0</item>
        <item name="android:textColor">@color/buttonRunTextColor</item>
    </style>
    <style name="ButtonPillLeftStyle" parent="ButtonPillStyle">
        <item name="shapeAppearanceOverlay">@style/ButtonPillLeftCornerRadiusStyle</item>
        <item name="backgroundTint">@color/button_run_bg_selector</item>
        <item name="android:minWidth">50dp</item>
    </style>
    <style name="ButtonPillMiddleStyle" parent="ButtonPillStyle">
        <item name="backgroundTint">@color/button_run_bg_selector</item>
        <item name="cornerRadius">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="ButtonPillRightStyle" parent="ButtonPillStyle">
        <item name="shapeAppearanceOverlay">@style/ButtonPillRightCornerRadiusStyle</item>
        <item name="backgroundTint">@color/button_run_bg_selector</item>
        <item name="android:minWidth">50dp</item>
    </style>
    <style name="ButtonPillLeftCornerRadiusStyle" parent="ShapeAppearance.Material3.Corner.None">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">5dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomLeft">5dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>
    <style name="ButtonPillRightCornerRadiusStyle" parent="ShapeAppearance.Material3.Corner.None">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">5dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">5dp</item>
    </style>



    <style name="EditTextResult">
