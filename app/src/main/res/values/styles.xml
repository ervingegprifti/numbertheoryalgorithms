    <!-- Clipboard Button Styles -->
    <style name="ClipboardButtonStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_clipboard_bg</item>
        <item name="android:textColor">@color/clipboardButtonText</item>
        <item name="android:fontFamily">@font/fa_solid_900</item>
        <item name="android:paddingHorizontal">15dp</item>
        <item name="android:paddingVertical">8dp</item>
    </style>
    <style name="ClipboardButtonExpandResultStyle" parent="ClipboardButtonStyle">
        <item name="android:layout_marginRight">5dp</item>
        <!--<item name="android:text">@string/expand_result</item>-->
        <item name="android:text">@string/fa_expand</item>
        <item name="android:visibility">gone</item>
    </style>
    <style name="ClipboardButtonCopyStyle" parent="ClipboardButtonStyle">
        <item name="android:layout_marginRight">5dp</item>
        <!--<item name="android:text">@string/clipboard_copy</item>-->
        <item name="android:text">@string/fa_copy</item>
    </style>
    <style name="ClipboardButtonPasteStyle" parent="ClipboardButtonStyle">
        <item name="android:layout_marginRight">5dp</item>
        <!--<item name="android:text">@string/clipboard_paste</item>-->
        <item name="android:text">@string/fa_paste</item>
    </style>
    <style name="ClipboardButtonClearStyle" parent="ClipboardButtonStyle">
        <item name="android:layout_marginRight">0dp</item>
        <!--<item name="android:text">@string/clipboard_clear</item>-->
        <item name="android:text">@string/fa_eraser</item>
    </style>


    <!--Compact inputGroup view styles-->
    <style name="CompactStyle">
        <item name="android:layout_height">40dp</item> <!--TODO +++ Work with this-->
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <style name="TextViewLabelCompactStyle" parent="CompactStyle">
        <item name="android:layout_width">30dp</item>
        <item name="android:background">@drawable/label_input_compact_bg</item>
        <item name="android:textColor">@color/colorLabel</item>
    </style>
    <style name="EditTextNumberInputCompactStyle" parent="CompactStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/edittext_input_compact_bg</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:inputType">numberSigned</item>
        <item name="android:digits">-0123456789</item>
        <item name="android:maxLength">1000000</item>
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:paddingHorizontal">10dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">false</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="ClipboardButtonMiddleCompactStyle" parent="CompactStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@drawable/button_clipboard_middle_compact_bg</item>
        <item name="android:textColor">@color/clipboardButtonText</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:paddingHorizontal">15dp</item>
        <item name="android:fontFamily">@font/fa_solid_900</item>
    </style>
    <style name="ClipboardButtonRightCompactStyle" parent="CompactStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@drawable/button_clipboard_right_compact_bg</item>
        <item name="android:textColor">@color/clipboardButtonText</item>
        <item name="android:paddingHorizontal">15dp</item>
        <item name="android:fontFamily">@font/fa_solid_900</item>
    </style>
    <style name="ClipboardButtonCopyMiddleCompactStyle" parent="ClipboardButtonMiddleCompactStyle">
<!--        <item name="android:text">@string/clipboard_copy</item>-->
        <item name="android:text">@string/fa_copy</item>
    </style>
    <style name="ClipboardButtonPasteMiddleCompactStyle" parent="ClipboardButtonMiddleCompactStyle">
        <!--        <item name="android:text">@string/clipboard_paste</item>-->
        <item name="android:text">@string/fa_paste</item>
    </style>
    <style name="ClipboardButtonClearMiddleCompactStyle" parent="ClipboardButtonRightCompactStyle">
        <!--        <item name="android:text">@string/clipboard_clear</item>-->
        <item name="android:text">@string/fa_eraser</item>
    </style>




